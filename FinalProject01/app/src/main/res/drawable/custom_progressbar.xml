<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item android:id="@+id/man_state_progress">
        <shape>
            <corners android:radius="6dp"/>
            <solid android:color="@color/purple_700"/>
        </shape>
    </item>
    <item android:id="@+id/woman_state_progress">
        <clip>
            <shape>
                <corners android:radius="6dp"/>
                <solid android:color="@color/pin_background"/>
            </shape>
        </clip>
    </item>

</layer-list>